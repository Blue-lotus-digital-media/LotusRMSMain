
@model int
        <h3>
        Contact Person @{
            var i = Model;
            var g = i + 1;
            }
            @g
        </h3>
        <div class="row">
            <div class="col-12">
                <label class="form-label">Contact Person Name</label>
                <input type="text" class="form-control" name="ContactPerson[@i].PersonName">
            </div>
            <div class="col-12">
                <label class="form-label">Phone Number</label>
                <input type="number" class="form-control" name="ContactPerson[@i].ContactNumber">
            </div>
            <div class="col-12">
                <label class="form-label">Address</label>
                <input type="number" class="form-control" name="ContactPerson[@i].Address">
            </div>
            </div>
    

   

